{
  "name": "kamaji",
  "module": "index.tsx",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "bun run build.ts",
    "postbuild": "bun run relative-pathing",
    "build:watch": "bun run build.ts --watch",
    "dev": "bun run build && bun run --watch index.tsx",
    "start": "NODE_ENV=production bun run dist/index.js",
    "relative-pathing": "./node_modules/relative-pathing/bin/relative-pathing --in ./public/projects-showcase --out ./public/projects-showcase --rules ./relative-pathing.json"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "hono": "^4.10.8",
    "relative-pathing": "github:ed-yahska-xyz/relative-pathing#main"
  },
  "trustedDependencies": [
    "relative-pathing"
  ]
}
